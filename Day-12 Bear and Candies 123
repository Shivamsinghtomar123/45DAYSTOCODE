#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int a,b;
	    cin>>a>>b;
	    int k=1;
	    
	    while(a>=0 && b>=0){
	        
	        if(k%2!=0) a-=k;
	        else b-=k;
	        k++;
	    }
	    
	    if(a>b) cout<<"Limak\n";
	    else cout<<"Bob\n";
	}
	    
	
	return 0;
}
